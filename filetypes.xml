<application>
  <component name="FileTypeManager" version="16">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <removed_mapping ext="htm" type="HTML" />
      <mapping pattern="README" type="PLAIN_TEXT" />
      <mapping ext="dat" type="PLAIN_TEXT" />
      <mapping ext="dwt" type="Smarty" />
      <mapping ext="htm" type="Twig" />
    </extensionMap>
  </component>
</application>